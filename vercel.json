

{

 "scripts": {
      "start": "node_modules/next/dist/bin/next start -p $PORT"
    },
"version":2,
"builds":[

   {
    "src":"ec/wsgi.py",
    "use":"vercel/python",
    "config":{"maxLambdaSize" :"15mb","runtime":"python3.9"}
   },
   {
    "src":"build_files.sh",
    "use":"@vercel/static-build",
    "config":{"distDir": "staticfiles_build"}

   }
],
"routes":[
    {
   "src" :"/static/(.*)",
   "dest":"/static/$1"
},

{
    "src" :"/(.*)",
    "dest":"ec/wsgi.py"
 }
]
}


